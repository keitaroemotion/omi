curl https://api.omise.co/recipients \
  -X POST \
  -u $skey \
  -d "name=test recipient" \
  -d "email=john.doe@example.com" \
  -d "description=John Doe" \
  -d "type=individual" \
  -d "bank_account[account_type]=normal" \
  -d "bank_account[bank_code]=0001" \
  -d "bank_account[branch_code]=001" \
  -d "bank_account[number]=0000001" \
  -d "bank_account[name]=Test bank account"
